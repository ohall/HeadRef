<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
	</s:states>
	<s:BorderContainer borderAlpha="0" borderAlpha.down="1"
					   borderColor="{RefConstants.FLASHGREEN}" height="18">
		<s:BitmapImage  source="@Embed('../style/skins/endgame.png')" verticalCenter="0"/>		
	</s:BorderContainer>
</s:Skin>
