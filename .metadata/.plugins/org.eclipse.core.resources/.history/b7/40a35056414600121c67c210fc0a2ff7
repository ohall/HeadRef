package model{
	public class GameModel{
		
		
		[Bindable] public var strikes:Number;
		[Bindable] public var balls:Number;
		[Bindable] public var fouls:Number;
		[Bindable] public var outs:Number;
		[Bindable] public var innings:Number;
		
		[Bindable] public var homeTeam:String;
		[Bindable] public var homeTeamColor:uint;
		[Bindable] public var homeTeamScore:Number;
		
		[Bindable] public var awayTeam:String;
		[Bindable] public var awayTeamColor:uint;
		[Bindable] public var awayTeamScore:Number;
		
		[Bindable] public var leagueRules:LeagueRules;
		
		
		public function GameModel(){}
	}
}